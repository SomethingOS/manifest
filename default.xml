<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <!-- AOSP -->
  <remote name="aosp"
          fetch="https://android.googlesource.com"
          sync-c="true"
          sync-tags="false"
          review="https://android-review.googlesource.com/"
          revision="refs/tags/android-15.0.0_r1" />

  <!-- CLO -->
  <remote name="clo-la"
          fetch="https://git.codelinaro.org/clo/la"
          sync-c="true"
          sync-tags="false" />

  <default remote="clo-la"/>

  <include name="system.xml"/>
  <include name="vendor.xml"/>

  <!-- Something -->
  <remote  name="something-devices"
           fetch="https://github.com/SomethingOS-Devices"
           revision="uvite"/>

  <remote  name="something-devices-prv"
           fetch="ssh://git@github.com/SomethingOS-Devices"
           revision="uvite"/>

  <remote  name="something"
           fetch="https://github.com/SomethingOS"
           revision="uvite"/>

  <remote  name="aospa"
           fetch=".."
           review="gerrit.aospa.co"
           revision="uvite"/>

  <remote  name="bitbucket"
           fetch="https://bitbucket.org"
           revision="uvite" />

  <remote  name="blobs"
           fetch="https://github.com/ThankYouMario"
           revision="uvite" />

  <remote  name="blobs-gitlab"
           fetch="https://gitlab.com/ThankYouMario"
           revision="uvite" />

  <remote name="blobs-something"
           fetch="https://gitlab.com/somethingos"
           revision="uvite" />

  <remote  name="github"
           fetch="https://github.com" />

  <remote  name="github-ssh"
           fetch="ssh://git@github.com" />

  <remote  name="gitlab"
           fetch="https://gitlab.com" />

  <include name="aosp.xml"/>
  <include name="aospa.xml"/>
  <include name="something.xml"/>
  <include name="obsolete.xml"/>

</manifest>
